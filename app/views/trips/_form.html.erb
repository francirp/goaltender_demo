<%= form_for(@trip) do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset>
    <%= f.label :start_date %>
    <%= f.text_field :start_date, class: 'datepicker' %>
  </fieldset>
  <fieldset>
    <%= f.label :end_date %>
    <%= f.text_field :end_date %>
  </fieldset>
<% end %>
